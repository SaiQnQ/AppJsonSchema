{
  "teams": {
    "creation_engine": {
      "name": "Creation Engine Team",
      "responsibilities": [
        "Project scaffolding",
        "Component generation",
        "Template management"
      ],
      "directories": {
        "frontend": [
          "src/generators",
          "src/templates",
          "src/builders"
        ],
        "backend": [
