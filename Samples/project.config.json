{
  "project": {
    "name": "Enterprise Dashboard",
    "version": "1.0.0",
    "description": "Enterprise-grade application management system",
    "generator_version": "1.0.0",
    "template_engine": "next.js",
    "api_framework": "fastapi",
    "database": "postgresql",
    "dependencies": {
      "frontend": {
        "next": "14.1.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "tailwindcss": "3.4.1",
        "@headlessui/react": "1.7.18",
        "@heroicons/react": "2.1.1",
        "axios": "1.6.7",
        "formik": "2.4.5",
        "yup": "1.3.3",
        "zustand": "4.5.0",
        "react-query": "5.0.0"
      },
      "backend": {
        "fastapi": "0.109.0",
        "sqlalchemy": "2.0.25",
        "pydantic": "2.5.3",
        "alembic": "1.13.1",
        "python-jose[cryptography]": "3.3.0",
        "passlib[bcrypt]": "1.7.4",
        "python-multipart": "0.0.6",
        "psycopg2-binary": "2.9.9"
      }
    },
    "structure": {
      "frontend": {
        "src": {
          "components": ["forms", "tables", "dashboard", "navigation", "common"],
          "hooks": ["auth", "data", "ui"],
          "services": ["api", "auth", "storage"],
          "store": ["slices", "middleware"],
          "styles": ["globals", "components"],
          "utils": ["validation", "formatting", "helpers"]
        }
      },
      "backend": {
        "app": {
          "api": ["v1"],
          "core": ["config", "security", "database"],
          "models": ["domain", "schemas"],
          "services": ["auth", "users", "files"],
          "utils": ["email", "validation"]
        }
      }
    },
    "deployment": {
      "docker": true,
      "docker_compose": true,
      "env_variables": [
        {
          "name": "DATABASE_URL",
          "required": true,
          "description": "PostgreSQL connection string"
        },
        {
          "name": "SECRET_KEY",
          "required": true,
          "description": "JWT secret key"
        },
        {
          "name": "SMTP_HOST",
          "required": false,
          "description": "SMTP server for emails"
        }
      ]
    }
  }
} 